
<br><h1>Self Defense Wepeaons</h1>
<br>


<div class="row">
  <% for(var i=0;i<products.length;i++) {%>
    <div class="col-4" style="padding: 3%;">
     
    
      <div><h5><b><%=products[i].name %></b></h5></div>
      <div>
        <img
            src="/images/uploaded/<%=products[i].image %>"
            alt=""
            width="70%"
          />
      </div>
      <div>
        <div>
          <p><b><%=products[i].description %></b></p>
          <p><b>Price: </b><%=products[i].price %></p>
          <p><b>Color: </b><%=products[i].color %></p>
        </div>
        <a href="/add-cart/<%=products[i]._id %>" class="btn btn-info w-full"
          >Add To Cart</a
        >
        
      </div>
    </div>
    <% } %>

  </products.length;i++)>
</div>
</div>
